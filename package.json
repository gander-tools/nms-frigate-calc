    "@types/bootstrap": "^5.2.10",
    "@types/node": "^22.10.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.6.0",
    "npm-run-all2": "^7.0.1",
    "release-it": "^17.10.0",
    "sass": "^1.81.0",
    "typescript": "5.6.3",
    "vite": "5.4.6",
    "vite-plugin-html-config": "^2.0.2",
